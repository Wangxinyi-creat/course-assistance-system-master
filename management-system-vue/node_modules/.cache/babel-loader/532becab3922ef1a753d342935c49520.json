  <i class=\"el-icon-d-arrow-left\" style=\"cursor: pointer;font-size: 20px\" @click=\"goBack()\">返回</i>\n      <p style=\"text-align: center;font-size: 26px\">课程：{{ course.courseName }}</p>\n      <el-tabs v-model=\"activeName\" class=\"tabs-style\">\n        <el-tab-pane label=\"基本信息\" name=\"first\">\n          <el-descriptions :column=\"1\">\n            <el-descriptions-item label=\"课程名称\">{{ course.courseName }}</el-descriptions-item>\n            <el-descriptions-item label=\"教师\">{{ course.teacherName }}</el-descriptions-item>\n            <el-descriptions-item label=\"上课时间\"> {{ getClassTime(course.classTime) }}</el-descriptions-item>\n            <el-descriptions-item label=\"课程类型\">{{ course.courseType }}</el-descriptions-item>\n            <el-descriptions-item label=\"课程介绍\">{{ course.courseDescription }}</el-descriptions-item>\n            <el-descriptions-item label=\"课程状态\">\n              <el-tag v-if=\"course.status === 1\" type=\"success\">选课中</el-tag>\n              <el-tag v-else-if=\"course.status === 2\">教学中</el-tag>\n              <el-tag v-else type=\"info\">已结束</el-tag>\n            </el-descriptions-item>\n          </el-descriptions>\n        </el-tab-pane>\n        <el-tab-pane label=\"学习资源查看\" name=\"second\">\n          <resource :course-id=\"courseId\"></resource>\n        </el-tab-pane>\n        <el-tab-pane label=\"作业查看\" name=\"fourth\">\n          <experiment :course-id=\"courseId\"></experiment>\n        </el-tab-pane>\n      </el-tabs>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport {getDetail} from \"@/api/scas/course\";\n\nexport default {\n  name: 'CoursesDetail.vue',\n  data() {\n    return {\n      courseId: '',\n      course: {},\n      activeName: 'first'\n    };\n  },\n  components: {\n    experiment: () => import('./experiment/index.vue'),\n    resource: () => import('./resource/index.vue'),\n  },\n  created() {\n    this.courseId = this.$route.query.id\n    this.getDetail()\n  },\n  methods: {\n    getDetail() {\n      getDetail(this.courseId).then(res => {\n        this.course = res.data\n      })\n    },\n    goBack() {\n      this.$router.go(-1)\n    },\n    getClassTime(item) {\n      const data = JSON.parse(item)\n      const convertedData = {};\n\n      // 遍历传入的 JSON 形式的数据数组\n      data.forEach(entry => {\n        // 将 \"dictLabel\" 作为键， \"dictValue\" 作为值，添加到转换后的对象中\n        convertedData[entry.dictLabel] = entry.dictValue;\n      });\n\n      // 将转换后的对象转换为 JSON 格式的字符串\n      return JSON.stringify(convertedData)\n          // 使用正则表达式替换双引号和逗号\n          .replace(/\"/g, '') // 去掉双引号\n          .replace(/,/g, '； ')// 添加空格;\n          .slice(1, -1);\n    },\n  }\n};\n</script>\n\n<style>\n.tabs-style {\n  padding: 20px;\n}\n\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgCA,SAAAA,SAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,MAAA;MACAC,UAAA;IACA;EACA;EACAC,UAAA;IACAC,UAAA,EAAAA,CAAA;IACAC,QAAA,EAAAA,CAAA;EACA;EACAC,QAAA;IACA,KAAAN,QAAA,QAAAO,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA,KAAAZ,SAAA;EACA;EACAa,OAAA;IACAb,UAAA;MACAA,SAAA,MAAAG,QAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,KAAAX,MAAA,GAAAW,GAAA,CAAAb,IAAA;MACA;IACA;IACAc,OAAA;MACA,KAAAC,OAAA,CAAAC,EAAA;IACA;IACAC,aAAAC,IAAA;MACA,MAAAlB,IAAA,GAAAmB,IAAA,CAAAC,KAAA,CAAAF,IAAA;MACA,MAAAG,aAAA;;MAEA;MACArB,IAAA,CAAAsB,OAAA,CAAAC,KAAA;QACA;QACAF,aAAA,CAAAE,KAAA,CAAAC,SAAA,IAAAD,KAAA,CAAAE,SAAA;MACA;;MAEA;MACA,OAAAN,IAAA,CAAAO,SAAA,CAAAL,aAAA;MACA;MAAA,CACAM,OAAA;MAAA,CACAA,OAAA;MAAA,CACAC,KAAA;IACA;EACA;AACA"}]}