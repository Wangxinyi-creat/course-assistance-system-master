-to-body>\n    <el-upload\n        class=\"upload-demo\"\n        drag\n        :auto-upload=\"false\"\n        action=\"#\"\n        :limit=\"1\"\n        :file-list=\"fileList\"\n        :on-change=\"handleChanges\"\n        :on-exceed=\"handleExceed\"\n        :on-remove=\"handleRemove\"\n        multiple>\n      <i class=\"el-icon-upload\"></i>\n      <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\n      <div class=\"el-upload__tip\" slot=\"tip\">只能上传1个文件,重新上传请先删除</div>\n    </el-upload>\n    <div slot=\"footer\" class=\"dialog-footer\">\n      <el-button type=\"primary\" @click=\"handleImportExcel\" :disabled=\"uploading\">确 定</el-button>\n      <el-button @click=\"cancel\">取 消</el-button>\n    </div>\n  </el-dialog>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"UploadExcel\",\n  data() {\n    return {\n      open: false,\n      title: '上传文件',\n      fileList: [],  //文件列表\n      file: {},\n      uploading: false,\n      baseURL: process.env.VUE_APP_APIUPLOAD\n    }\n  },\n  methods: {\n    /**\n     * 文件状态改变触发\n     * @param file  当前文件\n     * @param fileList 当前文件列表\n     */\n    handleChanges(file, fileList) {\n      this.uploading = false;\n      this.file = file;\n    },\n    /**\n     * 文件超出个数限制\n     */\n    handleExceed() {\n      this.$message.warning(\"仅允许上传一个文件！\");\n    },\n    /**\n     * 移除文件\n     */\n    handleRemove() {\n      this.uploading = true;\n    },\n    /**\n     * 上传\n     */\n    handleImportExcel() {\n      if (!this.file.size) {\n        this.$message.warning(\"请选择上传的文件\");\n        return;\n      }\n      this.uploading = true\n      let formData = new FormData();\n      formData.append(\"file\", this.file.raw);\n      request.post(this.baseURL, formData).then(res => {\n        this.uploading = false\n        this.$message.success(\"上传成功！\")\n        this.open = false\n        this.$emit('uploadRes',res)\n      }).catch(() => {\n        this.uploading = false\n      })\n    },\n    cancel() {\n      this.open = false\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n.el-input {\n  width: 90%;\n}\n\n.el-form .el-select {\n  width: 90%;\n}\n\n.upload-demo {\n  text-align: center;\n  justify-content: center;\n}\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;AAyBA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,QAAA;MAAA;MACAC,IAAA;MACAC,SAAA;MACAC,OAAA,EAAAC,OAAA,CAAAC,GAAA,CAAAC;IACA;EACA;EACAC,OAAA;IACA;AACA;AACA;AACA;AACA;IACAC,cAAAP,IAAA,EAAAD,QAAA;MACA,KAAAE,SAAA;MACA,KAAAD,IAAA,GAAAA,IAAA;IACA;IACA;AACA;AACA;IACAQ,aAAA;MACA,KAAAC,QAAA,CAAAC,OAAA;IACA;IACA;AACA;AACA;IACAC,aAAA;MACA,KAAAV,SAAA;IACA;IACA;AACA;AACA;IACAW,kBAAA;MACA,UAAAZ,IAAA,CAAAa,IAAA;QACA,KAAAJ,QAAA,CAAAC,OAAA;QACA;MACA;MACA,KAAAT,SAAA;MACA,IAAAa,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,cAAAhB,IAAA,CAAAiB,GAAA;MACAvB,OAAA,CAAAwB,IAAA,MAAAhB,OAAA,EAAAY,QAAA,EAAAK,IAAA,CAAAC,GAAA;QACA,KAAAnB,SAAA;QACA,KAAAQ,QAAA,CAAAY,OAAA;QACA,KAAAxB,IAAA;QACA,KAAAyB,KAAA,cAAAF,GAAA;MACA,GAAAG,KAAA;QACA,KAAAtB,SAAA;MACA;IACA;IACAuB,OAAA;MACA,KAAA3B,IAAA;IACA;EACA;AAEA"}]}