  <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"700px\" append-to-body>\n    <el-upload\n      class=\"upload-demo\"\n      drag\n      :auto-upload=\"false\"\n      action=\"#\"\n      :limit=\"1\"\n      :file-list=\"fileList\"\n      :on-change=\"handleChanges\"\n      :on-exceed=\"handleExceed\"\n      :on-remove=\"handleRemove\"\n      accept=\".xls,.xlsx\"\n      multiple>\n      <i class=\"el-icon-upload\"></i>\n      <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\n      <div class=\"el-upload__tip\" slot=\"tip\">只能上传1个excel文件,重新上传请先删除</div>\n      <el-button type=\"text\" slot=\"tip\" @click=\"exportTemplate\">点击下载导入模板</el-button>\n    </el-upload>\n    <div slot=\"footer\" class=\"dialog-footer\">\n      <el-button type=\"primary\" @click=\"handleImportExcel\" :disabled=\"uploading\">确 定</el-button>\n      <el-button @click=\"cancel\">取 消</el-button>\n    </div>\n  </el-dialog>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"UploadExcel\",\n  data() {\n    return {\n      open: false,\n      title: '导入',\n      fileList: [],  //文件列表\n      file: {},\n      uploading: false,\n      baseURL: process.env.VUE_APP_APIURL\n    }\n  },\n  props: {\n    url: String,//上传地址\n    fileName: String//模板文件名\n  },\n  methods: {\n    //下载模板  将模板文件放置在public/excel文件夹下\n    exportTemplate() {\n      const url = this.baseURL + \"excel/\" + this.fileName\n      window.open(url)\n    },\n    /**\n     * 文件状态改变触发\n     * @param file  当前文件\n     * @param fileList 当前文件列表\n     */\n    handleChanges(file, fileList) {\n      this.uploading = false;\n      this.file = file;\n    },\n    /**\n     * 文件超出个数限制\n     */\n    handleExceed() {\n      this.$message.warning(\"仅允许上传一个文件！\");\n    },\n    /**\n     * 移除文件\n     */\n    handleRemove() {\n      this.uploading = true;\n    },\n    /**\n     * 上传\n     */\n    handleImportExcel() {\n      if (!this.file.size) {\n        this.$message.warning(\"请选择上传的文件\");\n        return;\n      }\n      this.uploading = true\n      let formData = new FormData();\n      formData.append(\"file\", this.file.raw);\n      request.post(this.url, formData).then(res => {\n        this.uploading = false\n        this.$message.success(\"导入成功\")\n        this.open = false\n        this.$emit('refresh')\n      }).catch(() => {\n        this.uploading = false\n      })\n    },\n    cancel() {\n      this.open = false\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n.el-input {\n  width: 90%;\n}\n\n.el-form .el-select {\n  width: 90%;\n}\n\n.upload-demo {\n  text-align: center;\n  justify-content: center;\n}\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;AA2BA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,QAAA;MAAA;MACAC,IAAA;MACAC,SAAA;MACAC,OAAA,EAAAC,OAAA,CAAAC,GAAA,CAAAC;IACA;EACA;EACAC,KAAA;IACAC,GAAA,EAAAC,MAAA;IAAA;IACAC,QAAA,EAAAD,MAAA;EACA;;EACAE,OAAA;IACA;IACAC,eAAA;MACA,MAAAJ,GAAA,QAAAL,OAAA,mBAAAO,QAAA;MACAG,MAAA,CAAAf,IAAA,CAAAU,GAAA;IACA;IACA;AACA;AACA;AACA;AACA;IACAM,cAAAb,IAAA,EAAAD,QAAA;MACA,KAAAE,SAAA;MACA,KAAAD,IAAA,GAAAA,IAAA;IACA;IACA;AACA;AACA;IACAc,aAAA;MACA,KAAAC,QAAA,CAAAC,OAAA;IACA;IACA;AACA;AACA;IACAC,aAAA;MACA,KAAAhB,SAAA;IACA;IACA;AACA;AACA;IACAiB,kBAAA;MACA,UAAAlB,IAAA,CAAAmB,IAAA;QACA,KAAAJ,QAAA,CAAAC,OAAA;QACA;MACA;MACA,KAAAf,SAAA;MACA,IAAAmB,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,cAAAtB,IAAA,CAAAuB,GAAA;MACA7B,OAAA,CAAA8B,IAAA,MAAAjB,GAAA,EAAAa,QAAA,EAAAK,IAAA,CAAAC,GAAA;QACA,KAAAzB,SAAA;QACA,KAAAc,QAAA,CAAAY,OAAA;QACA,KAAA9B,IAAA;QACA,KAAA+B,KAAA;MACA,GAAAC,KAAA;QACA,KAAA5B,SAAA;MACA;IACA;IACA6B,OAAA;MACA,KAAAjC,IAAA;IACA;EACA;AAEA"}]}