public class LongestSubstringWithoutRepeatingCharacters {    public static void main(String[] args) {        String s = "abcabcbb";        Map<Character, Integer> map = new HashMap<>();        int max = 0;        int start = 0;        for (int end = 0; end < s.length(); end++) {            if (map.containsKey(s.charAt(end))) {                start = Math.max(start, map.get(s.charAt(end)) + 1);            }            map.put(s.charAt(end), end);            max = Math.max(max, end - start + 1);        }        System.out.println(max);    }}